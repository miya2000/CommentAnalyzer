<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>CommentAnalyzer</id>
    <version>1.0.0.0</version>
    <title>CommentAnalyzer</title>
    <authors>miya2000</authors>
    <owners>miya2000</owners>
    <licenseUrl>https://github.com/miya2000/CommentAnalyzer</licenseUrl>
    <projectUrl>https://github.com/miya2000/CommentAnalyzer</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Analyze comments and code fix.</description>
    <releaseNotes>First Major Release.</releaseNotes>
    <copyright>Copyright</copyright>
    <tags>CommentAnalyzer, analyzers, comment</tags>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\dotnet and language specific analyzers in either analyzers\dotnet\cs or analyzers\dotnet\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*" />
    <file src="tools\*.ps1" target="tools\" />
    <file src="ja-JP\*.dll" target="analyzers\dotnet\cs\ja-JP" />
  </files>
</package>